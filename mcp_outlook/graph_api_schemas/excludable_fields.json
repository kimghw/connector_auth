    },
    "isReadReceiptRequested": {
      "type": "Boolean",
      "operator": "ne",
      "description": "읽음 확인 요청 제외",
      "input": "제외할 상태 (true/false)",
      "result": "지정한 상태가 아닌 메일만 조회"
    },
    "flag/flagStatus": {
      "type": "String",
      "operator": "ne",
      "enum": ["notFlagged", "complete", "flagged"],
      "description": "특정 플래그 상태 제외",
      "input": "제외할 플래그 상태",
      "result": "해당 플래그 상태가 아닌 메일만 조회"
    },
    "categories": {
      "type": "Collection<String>",
      "operator": "not any",
      "description": "특정 카테고리 제외",
      "input": "제외할 카테고리 이름",
      "result": "해당 카테고리가 없는 메일만 조회"
    },
    "id": {
      "type": "String",
      "operator": "ne",
      "description": "특정 메일 ID 제외",
      "input": "제외할 메일 ID",
      "result": "해당 ID가 아닌 모든 메일 조회"
    },
    "conversationId": {
      "type": "String",
      "operator": "ne",
      "description": "특정 대화 스레드 제외",
      "input": "제외할 대화 ID",
      "result": "해당 대화 스레드가 아닌 메일만 조회"
    },
    "parentFolderId": {
      "type": "String",
      "operator": "ne",
      "description": "특정 폴더 제외",
      "input": "제외할 폴더 ID",
      "result": "해당 폴더가 아닌 메일만 조회"
    }
  }
}