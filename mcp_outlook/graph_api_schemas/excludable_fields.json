{
  "excludableFields": {
    "from/emailAddress/address": {
      "type": "String",
      "operator": "ne",
      "description": "특정 발신자 제외",
      "input": "제외할 이메일 주소",
      "result": "해당 주소에서 보낸 메일 제외"
    },
    "sender/emailAddress/address": {
      "type": "String",
      "operator": "ne",
      "description": "특정 실제 발송자 제외",
      "input": "제외할 발송자 주소",
      "result": "해당 주소가 발송한 메일 제외"
    },
    "subject": {
      "type": "String",
      "operator": "not contains",
      "description": "제목에 특정 키워드 없는 메일",
      "input": "제외할 키워드",
      "result": "제목에 해당 키워드가 없는 메일만 조회"
    },
    "body/content": {
      "type": "String",
      "operator": "not contains",
      "description": "본문에 특정 키워드 없는 메일",
      "input": "제외할 키워드",
      "result": "본문에 해당 키워드가 없는 메일만 조회"
    },
    "bodyPreview": {
      "type": "String",
      "operator": "not contains",
